{
	"flask": {
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1
	},
	"treeherder": {
		"use_cache": true,
		"rate_limit": 55,
		"cache": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "treeherder",
			"type": "job",
			"timeout": 300,
			"debug": false,
			"limit_replicas": false
		}
	},
	"hg": {
		"use_cache": true,
		"hg": {
			"url": "https://hg.mozilla.org"
		},
		"branches": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "branches",
			"type": "branch",
			"timeout": 300,
			"schema": {
				"$ref": "../../mohg/resources/branch.json"
			},
			"debug": false,
			"limit_replicas": true
		},
		"repo": {
			"host": "http://activedata.allizom.org",
			"port": 9200,
			"index": "repo",
			"type": "revision",
			"timeout": 300,
			"schema": {
				"$ref": "../../mohg/resources/revision.json"
			},
			"debug": false,
			"limit_replicas": false
		}
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://github.com/klahnakoski/MoTreeherder",
			"User-Agent": "MoTreeherder"
		}
	},
	"debug":{

	}
}
