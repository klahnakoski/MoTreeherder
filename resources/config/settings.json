{
	"flask":{
		"host": "0.0.0.0",
		"port": 5000,
		"debug": false,
		"threaded": true,
		"processes": 1
	},
	"treeherder": {
		"use_cache": true,
		"rate_limit": 55,
		"cache": {
			"host": "http://localhost",
			"port": 9200,
			"index": "treeherder",
			"type": "job",
			"timeout": 300,
			"debug": false,
			"limit_replicas": false
		}
	},
	"hg": {
		"$ref":"//../../mohg/resources/settings.json"
	},
	"constants": {
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://github.com/klahnakoski/MoTreeherder",
			"User-Agent": "MoTreeherder"
		}
	},
	"debug":{

	}
}
