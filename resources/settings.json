{
	"use_cache": true,
	"elasticsearch": {
		"host": "http://localhost",
		"port": 9200,
		"index": "treeherder",
		"type": "job",
		"timeout": 300,
		"schema": {
			"$ref": "//treeherder.json"
		},
		"debug": false,
		"limit_replicas": false
	},
	"constants":{
		"pyLibrary.env.http.default_headers": {
			"Referer": "https://github.com/klahnakoski/MoTreeherder",
			"User-Agent": "MoTreeherder"
		}
	}
}
